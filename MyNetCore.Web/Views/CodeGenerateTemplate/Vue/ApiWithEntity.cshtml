@model MyNetCore.Model.CodeGenerate.EntityPropertys
@{
    Layout = null;
}
/**
*┌──────────────────────────────────────────────────────────────┐
*│　描    述：@(Model.ModelDesc)Api
*│　作    者：@Model.Author
*│　版    本：1.0 使用Razor引擎自动生成
*│　创建时间：@Model.GeneratorTime
*└──────────────────────────────────────────────────────────────┘
*/

import request from "@@/util/request"

//获取列表
export const apiGet@(Model.ModelName)PageList = data => {
    return request({
        url: '/@(Model.ModelVariableName)/get/pagelist',
        method: 'post',
        data
    })
}


//删除
export const apiDelete@(Model.ModelName)ByIds = (ids) => {
    return request({
        url: '/@(Model.ModelVariableName)/delete',
        method: 'delete',
        params: { ids: ids }
    })
}

//获取详情
export const apiGet@(Model.ModelName)Info = (id) => {
    return request({
        url: '/@(Model.ModelVariableName)/get/info',
        method: 'get',
        params: { id: id }
    })
}

//编辑信息
export const apiModify@(Model.ModelName)Info = data => {
    return request({
        url: '/@(Model.ModelVariableName)/modify',
        method: 'post',
        data
    })
}
